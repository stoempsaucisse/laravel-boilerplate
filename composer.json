{
    "name": "stoempsaucisse/laravel-boilerplate",
    "description": "A suite of boilerplate Artisan commands",
    "keywords": ["CRM", "ERP", "stoempsaucisse", "laravel", "artisan", "boilerplate"],
    "type": "package",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Stoemp Saucisse",
            "email": "stoempsaucisse@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.8",
        "illuminate/support": "^5.0",
        "illuminate/database": "~5.0",
        "illuminate/console": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/log": "~5.0",
        "illuminate/filesystem": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Stoempsaucisse\\": "src/Stoempsaucisse/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stoempsaucisse\\LaravelBoilerplate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": "Stoempsaucisse\\LaravelBoilerplate\\ComposerScripts::cpArtisan"
    },
    "minimum-stability": "stable"
}
